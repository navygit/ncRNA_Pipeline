[SubmitProteome]
module=Dummy
input_id_type=PROTEOME

[SubmitChunk]
module=Dummy
input_id_type=CHUNK

[SubmitTranslation]
module=Dummy
input_id_type=TRANSLATIONID

[scanprosite]
db=Prosite_patterns
db_version=1
db_file=/nfs/seqdb/production/interpro/data/members/prosite/20.52/prosite.patterns
parameters=-confirm => /nfs/panda/ensemblgenomes/production/protein_pipelines/databases/confirm.patterns
module=ProteinAnnotation/PrositeProfile
gff_source=Prosite_pattern
gff_feature=domain
input_id_type=CHUNK

[ncoils]
db=coiled_coil
db_file=/nfs/panda/ensemblgenomes/external/src/coils
program=ncoils
program_file=/nfs/panda/ensemblgenomes/external/bin/ncoils
module=ProteinAnnotation/Coil
gff_source=ncoils
gff_feature=annotation
input_id_type=CHUNK

[tmhmm]
db=transmembrane
program=decodeanhmm
program_file=/nfs/panda/ensemblgenomes/external/TMHMM2.0c/bin/decodeanhmm
parameters=-modelfile => /nfs/panda/ensemblgenomes/external/TMHMM2.0c/lib/TMHMM2.0.model, -optionsfile => /nfs/panda/ensemblgenomes/external/TMHMM2.0c/lib/TMHMM2.0.options 
module=ProteinAnnotation/Tmhmm
gff_source=Tmhmm
gff_feature=annotation
input_id_type=CHUNK

[signalp]
db=signal_peptide
program=signalp
program_file=/nfs/panda/ensemblgenomes/external/signalp-4.0/signalp
program_version=4.0
module=ProteinAnnotation/Signalp
gff_source=Signalp
gff_feature=annotation
input_id_type=CHUNK

[pfscan]
db=Prosite_profiles
db_version=1
db_file=/nfs/panda/ensemblgenomes/production/protein_pipelines/databases/prosite.profiles
program=pfscan
program_file=/nfs/seqdb/production/interpro/binaries/64_bit_Linux/pfscan
module=ProteinAnnotation/PrositeProfile
gff_source=Profile
gff_feature=Domain
input_id_type=CHUNK

[prints]
db=Prints
db_version=1
db_file=/nfs/seqdb/production/interpro/data/iprscan/prints.pval
program=FingerPRINTScan
program_file=/nfs/seqdb/production/interpro/binaries/64_bit_Linux/fingerPRINTScan
module=ProteinAnnotation/Prints
gff_source=Prints
parameters=-e 0.0001 -d 10 -E 257043 84355444 -fjR -a -o 15
gff_feature=domain
input_id_type=CHUNK

[pfam]
db=Pfam
db_file=/ebi/production/interpro/data/members/pfam/23.0/Pfam_ls
db_version=26.0
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=--acc --cut_ga --cpu 1
module=ProteinAnnotation/Hmmpfam
gff_source=Pfam
gff_feature=domain
input_id_type=CHUNK

[seg]
db=low_complexity
program=seg
program_file=/nfs/panda/ensemblgenomes/external/bin/seg
module=ProteinAnnotation/Seg
gff_source=Seg
gff_feature=annotation
input_id_type=PROTEOME

[tigrfam]
db=TIGRfam
db_version=1
db_file=/ebi/sp/pro1/interpro/data/members/tigrfam/9.0/TIGRFAMs_9.0_HMM.LIB
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=--acc --cut_tc --cut_ga
module=ProteinAnnotation/Hmmpfam
gff_source=TIGRFAM
gff_feature=domain
input_id_type=CHUNK

[superfamily]
db=Superfamily
db_version=1
db_file=/nfs/seqdb/production/interpro/data/iprscan/superfamily.hmm
program=superfamily.pl
program_file=/nfs/panda/ensemblgenomes/production/protein_pipelines/programs/superfamily.pl
parameters=-t /nfs/nobackup/ensemblgenomes/production/protein_pipelines/temp/Superfamily -m /nfs/seqdb/production/interpro/data/iprscan/superfamily.hmm -d /nfs/seqdb/production/interpro/data/iprscan/superfamily.tab -a /nfs/seqdb/production/interpro/data/iprscan/superfamily.acc -p /sw/arch/bin/hmmpfam -s /nfs/panda/ensemblgenomes/production/protein_pipelines/programs 1e-05 -r y
module=ProteinAnnotation/Superfamily
gff_source=Superfamily
gff_feature=domain
input_id_type=CHUNK

[smart]
db=Smart
db_version=1
db_file=/nfs/seqdb/production/interpro/data/iprscan/smart.HMMs
program=hmmpfam
program_file=/sw/arch/bin/hmmpfam
parameters=-E 0.01 -A 100 -Z 350000 --acc --cpu 1 
module=ProteinAnnotation/Hmmpfam
gff_source=Smart
gff_feature=domain
input_id_type=CHUNK

[pirsf]
db=PIRSF
db_version=1
db_file=/nfs/seqdb/production/interpro/data/iprscan/sf_hmm;/nfs/seqdb/production/interpro/data/iprscan/sf_hmm_sub
program=pirsf.pl
program_file=/nfs/panda/ensemblgenomes/production/protein_pipelines/programs/pirsf.pl
parameters=-pirsf /ebi/sp/pro1/interpro/data/members/pirsf/275/pirsf.dat -sfhmm /nfs/seqdb/production/interpro/data/iprscan/sf_hmm.bin -subsf /net/isilon3/production/interpro/data/members/pirsf/275/sf_hmm_subf -sfseq /ebi/sp/pro1/interpro/data/members/pirsf/275/sf.seq -sftb /ebi/sp/pro1/interpro/data/members/pirsf/275/sf.tb -hmmer /sw/arch/bin/hmmpfam -blast /nfs/panda/ensemblgenomes/external/blast/bin/blastall
module=ProteinAnnotation/PIRSF
gff_source=PIRSF
gff_feature=domain
input_id_type=CHUNK

[hmmpanther]
db=PANTHER
db_version=1
gff_source=PANTHER
gff_feature=domain
input_id_type=CHUNK

[gene3d]
db=Gene3D
db_version=1
gff_source=Gene3D
gff_feature=domain
input_id_type=CHUNK

[blastprodom]
db=ProDom
db_version=1
gff_source=ProDom
